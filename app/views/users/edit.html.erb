<h1>Users#edit</h1>
<% if @user.errors.any? %>
    <%= @user.errors.count %>errors prohibited this obj from being saved:
    <ul>
        <% @user.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
    </ul>
<% end %>
<%= form_with model: @user do |f| %>
    <label for="inputName">Name</label>
    <%= f.text_field :name, autofocus: true, id:"inputName" %>

    <%= f.file_field :profile_image, accept: "image/jpg" %>

    <%= f.text_area :introduction, id:"inputIntroduction" %>

    <%= f.submit"Update User" %>
<% end %>
